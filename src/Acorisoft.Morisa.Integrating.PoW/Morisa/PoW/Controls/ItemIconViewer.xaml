    <Style TargetType="{x:Type this:ItemIconViewer}">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="MinHeight" Value="32" />
        <Setter Property="MinWidth" Value="32" />
        <Setter Property="SizeMode" Value="Normal" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type this:ItemIconViewer}">
                    <Border x:Name="PART_Bd"
                            Background="{Binding Mode=OneWay,RelativeSource={RelativeSource Mode=TemplatedParent},Path=Rarity,Converter={StaticResource Background}}" 
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Padding="{TemplateBinding Padding}"
                            CornerRadius="5">
                        <w:TilePanel>
                            <Image x:Name="PART_Decorative" Stretch="Uniform"/>
                            <Image x:Name="PART_Icon"
                                   Stretch="Uniform">
                                <Image.Source>
                                    <MultiBinding Converter="{StaticResource Icon}">
                                        <Binding Path="Category" Mode="OneWay" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                                        <Binding Path="Icon" Mode="OneWay" RelativeSource="{RelativeSource Mode=TemplatedParent}"/>
                                    </MultiBinding>
                                </Image.Source>
                            </Image>
                        </w:TilePanel>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="SizeMode" Value="Small">
                            <Setter Property="Margin" Value="16" TargetName="PART_Icon"/>
                        </Trigger>
                        <Trigger Property="SizeMode" Value="Normal">
                            <Setter Property="Margin" Value="32" TargetName="PART_Icon"/>
                            <Setter Property="CornerRadius" Value="5 5 0 0" TargetName="PART_Bd"/>
                        </Trigger>
                        <Trigger Property="SizeMode" Value="Large">
                            <Setter Property="Margin" Value="32" TargetName="PART_Icon"/>
                        </Trigger>
                        <Trigger Property="Rarity" Value="One">
                            <Setter Property="Source" TargetName="PART_Decorative" Value="{StaticResource Pow.Decorative.One}"/>
                        </Trigger>
                        <Trigger Property="Rarity" Value="One">
                        </Trigger>
                        <Trigger Property="Rarity" Value="Two">
                            <Setter Property="Source" TargetName="PART_Decorative" Value="{StaticResource Pow.Decorative.Two}"/>
                        </Trigger>
                        <Trigger Property="Rarity" Value="Three">
                            <Setter Property="Source" TargetName="PART_Decorative" Value="{StaticResource Pow.Decorative.Three}"/>
                        </Trigger>
                        <Trigger Property="Rarity" Value="Four">
                            <Setter Property="Source" TargetName="PART_Decorative" Value="{StaticResource Pow.Decorative.Four}"/>
                        </Trigger>
                        <Trigger Property="Rarity" Value="Five">
                            <Setter Property="Source" TargetName="PART_Decorative" Value="{StaticResource Pow.Decorative.Five}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
