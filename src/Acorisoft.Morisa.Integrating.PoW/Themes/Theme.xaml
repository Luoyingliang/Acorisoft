    <Style TargetType="{x:Type pow:AbilityItemView}">
        <Setter Property="Width" Value="192" />
        <Setter Property="Icon" Value="{StaticResource Pow.Sword.Cross.Image}" />
        <Setter Property="Height" Value="230" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pow:AbilityItemView}">
                    <Border CornerRadius="5" Background="{StaticResource Pow.White}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="160" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="1*" />
                            </Grid.RowDefinitions>
                            <Border Grid.Row="0" CornerRadius="5 5 0 0" Background="{TemplateBinding Rarity,Converter={x:Static pow:RarityBackgroundBrushConverter.Current}}">
                                <w:TilePanel>
                                    <Image x:Name="PART_Decorative" Source="{StaticResource Pow.Decorative.Five}">
                                        <Image.RenderTransform>
                                            <RotateTransform CenterX="89.5" CenterY="80"/>
                                        </Image.RenderTransform>
                                    </Image>
                                    <Image Source="{TemplateBinding Icon}" Stretch="Uniform" Margin="32"/>
                                </w:TilePanel>
                            </Border>

                            <pow:RarityView x:Name="PART_Rarity" Grid.Row="1" Rarity="{TemplateBinding Rarity}" Height="25"/>

                            <TextBlock Grid.Row="2" Text="{TemplateBinding AbilityName}" TextWrapping="Wrap" 
                                       FontSize="17" FontFamily="{StaticResource App.Fonts.Regular}" TextAlignment="Center" Margin="12 4 12 12" TextTrimming="CharacterEllipsis"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <!--<EventTrigger RoutedEvent="MouseEnter">
                            <BeginStoryboard Name="Rotation">
                                <Storyboard Storyboard.TargetName="PART_Decorative" Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)">
                                    <DoubleAnimation Duration="0:0:3" From="0" To="360" RepeatBehavior="Forever"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                        <EventTrigger RoutedEvent="MouseLeave">
                            <StopStoryboard BeginStoryboardName="Rotation"/>
                        </EventTrigger>-->
                        <Trigger Property="RarityValue" Value="One" SourceName="PART_Rarity">
