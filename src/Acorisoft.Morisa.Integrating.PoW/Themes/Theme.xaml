    <!--  -->
    <Style TargetType="{x:Type pow:AbilityEntryView}">
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pow:AbilityEntryView}">
                    <Border CornerRadius="5" Background="{StaticResource Pow.White}">
                        <DockPanel Margin="0 8" DockPanel.Dock="Bottom">
                            <TextBlock FontSize="16" Text="{TemplateBinding EntryName}" DockPanel.Dock="Left"/>
                            <TextBlock FontSize="16" 
                                       Foreground="#969696" 
                                       FontWeight="Light"
                                       Margin="8 0 0 0"
                                       Text="{TemplateBinding Description}" 
                                       DockPanel.Dock="Right" TextAlignment="Left" TextWrapping="Wrap"/>
                        </DockPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <!--  -->
    <Style TargetType="{x:Type pow:AbilityPartView}">
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pow:AbilityPartView}">
                    <Border CornerRadius="5" Background="{StaticResource Pow.White}">
                        <DockPanel Margin="0 8">
                            <TextBlock FontSize="20" Text="{TemplateBinding Title}" DockPanel.Dock="Top" Margin="0 10"/>
                            <ItemsPresenter Margin="0 8" DockPanel.Dock="Bottom"/>
                        </DockPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate DataType="{x:Type powDocs:AbilityEntry}">
                    <pow:AbilityEntryView Entry="{Binding}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>