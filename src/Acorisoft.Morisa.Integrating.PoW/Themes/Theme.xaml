    <Style TargetType="{x:Type pow:RarityView}">
        <Setter Property="Height" Value="32" />
        <Setter Property="MinWidth" Value="128" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type pow:RarityView}">
                    <Border>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <Path x:Name="One" Margin="3" Stretch="Uniform" Data="{StaticResource Pow.Star}" Fill="DarkOrange" Visibility="Collapsed"/>
                            <Path x:Name="Two" Margin="3" Stretch="Uniform" Data="{StaticResource Pow.Star}" Fill="DarkOrange" Visibility="Collapsed"/>
                            <Path x:Name="Three" Margin="3" Stretch="Uniform" Data="{StaticResource Pow.Star}" Fill="DarkOrange" Visibility="Collapsed"/>
                            <Path x:Name="Four" Margin="3" Stretch="Uniform" Data="{StaticResource Pow.Star}" Fill="DarkOrange" Visibility="Collapsed"/>
                            <Path x:Name="Five" Margin="3" Stretch="Uniform" Data="{StaticResource Pow.Star}" Fill="DarkOrange" Visibility="Collapsed"/>
                        </StackPanel>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="RarityValue" Value="One">
                            <Setter Property="Visibility" TargetName="One" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Two" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Three" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Four" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Five" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="RarityValue" Value="Two">
                            <Setter Property="Visibility" TargetName="One" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Two" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Three" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Four" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Five" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="RarityValue" Value="Three">
                            <Setter Property="Visibility" TargetName="One" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Two" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Three" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Four" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="Five" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="RarityValue" Value="Four">
                            <Setter Property="Visibility" TargetName="One" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Two" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Three" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Four" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Five" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="RarityValue" Value="Five">
                            <Setter Property="Visibility" TargetName="One" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Two" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Three" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Four" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="Five" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
